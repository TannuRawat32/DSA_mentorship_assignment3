class Solution 
{
    static Pair getMinMax(long a[], long n)  
    {
        long largest = Integer.MIN_VALUE;
        long smallest = Integer.MAX_VALUE;
        for(int i = 0; i<a.length; i++){
            if(largest<a[i]){
                largest = a[i];
            }
            if(smallest >a[i]){
                smallest = a[i];
            }
        }
        return new Pair(smallest, largest);
    }
}
